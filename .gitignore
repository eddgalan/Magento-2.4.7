# Ignorar configuraciones específicas del entorno
/.env.php
/nginx.conf.sample

# Ignorar carpetas de caché y logs
/var/cache/*
/var/composer_home/*
/var/generation/*
/var/page_cache/*
/var/view_preprocessed/*
/var/log/*
/var/report/*
/var/tmp/*
/var/session/*

# Ignorar archivos de compilación y dependencias generadas
/pub/static/*
!/pub/static/.htaccess
/generated/*
!/generated/.htaccess

# Ignorar extensiones locales y configuraciones
/app/etc/env.php
/app/etc/config.php

# Ignorar archivos de Composer y dependencias generadas
/vendor/*
/composer.lock

# Ignorar archivos de configuración de herramientas
/.idea
/.vscode
/.phpstorm.meta.php
/.buildpath
/.project
/.settings
/.toolstarget
/node_modules
/.sass-cache
/.pnp
/.pnp.js

# Ignorar archivos temporales del sistema operativo
*.swp
*.swo
.DS_Store
Thumbs.db

# Ignorar carpetas de prueba y ejemplos
/dev/tests/integration/tmp/*
/dev/tests/acceptance/tmp/*
/dev/tests/unit/tmp/*
/dev/tests/static/report/*
/dev/tests/static/tmp/*
/setup/config/application.config.php

# Ignorar archivos subidos por el usuario (media)
/pub/media/*
!/pub/media/.htaccess

# Ignorar archivos de seguridad y copias de respaldo
*.sql
*.tgz
*.gz
*.zip
*.tar

# Ignorar binarios
/bin/magento

/services/*
/var/.regenerate
